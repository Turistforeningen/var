{
  "name": "var",
  "version": "1.0.0",
  "description": "Registreringsskjema for frivillige",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && NODE_ENV=production webpack -p --progress --config ./webpack.config.prod.js",
    "lint": "eslint --ext .js,.jsx controllers/ lib/ src/ tests/",
    "lint:watch": "esw --color --ext .js,.jsx --watch controllers/ lib/ src/ tests/",
    "nsp": "nsp check",
    "start": "nodemon index.js",
    "test": "mocha -b -c -t 10000 --check-leaks -R tap -r test/*.js test/**/*.js",
    "test:watch": "mocha -w -b -c -t 10000 --check-leaks -R progress -r test/*.js test/**/*.js",
    "webpack:serve": "webpack-dev-server --env.development --color --config webpack.config.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Turistforeningen/var.git"
  },
  "author": "Den Norske Turistforening",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Turistforeningen/var/issues"
  },
  "homepage": "https://github.com/Turistforeningen/var#readme",
  "dependencies": {
    "body-parser": "^1.17.1",
    "express": "4.15.2",
    "isomorphic-fetch": "^2.2.1",
    "nodemon": "^1.11.0",
    "nunjucks": "^3.0.0",
    "raven": "1.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.3.26",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "core-decorators": "^0.17.0",
    "css-loader": "^0.27.3",
    "eslint": "^3.1.1",
    "eslint-config-airbnb-base": "^7.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-watch": "^2.1.14",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.9.0",
    "moment": "^2.18.1",
    "node-sass": "^4.5.2",
    "nsp": "^2.4.0",
    "raven-js": "^3.14.2",
    "react": "^15.4.2",
    "react-datetime": "^2.8.9",
    "react-dom": "^15.4.2",
    "react-hot-loader": "v3.0.0-beta.6",
    "react-redux": "^5.0.3",
    "react-router-dom": "^4.0.0",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.3.2",
    "webpack-dev-server": "2.4.2"
  }
}
