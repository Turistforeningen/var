{
  "name": "var",
  "version": "1.0.0",
  "description": "Registreringsskjema for frivillige",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ext .js,.jsx controllers/ lib/ src/ tests/",
    "lint:watch": "esw --color --ext .js,.jsx --watch controllers/ lib/ src/ tests/",
    "nsp": "nsp check",
    "start": "supervisor -w index.js,lib,controllers -e js index.js",
    "test": "mocha -b -c -t 10000 --check-leaks -R tap -r test/*.js test/**/*.js",
    "test:watch": "mocha -w -b -c -t 10000 --check-leaks -R progress -r test/*.js test/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Turistforeningen/var.git"
  },
  "author": "Den Norske Turistforening",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Turistforeningen/var/issues"
  },
  "homepage": "https://github.com/Turistforeningen/var#readme",
  "dependencies": {
    "body-parser": "^1.17.1",
    "eslint": "^3.1.1",
    "eslint-config-airbnb-base": "^7.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-watch": "^2.1.14",
    "express": "4.15.2",
    "nunjucks": "^3.0.0",
    "nsp": "^2.4.0",
    "raven": "1.2.0",
    "supervisor": "0.12.0"
  }
}
